    "@types/bun": "latest",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "prettier": "^3.0.0",
    "semantic-release": "^25.0.2",
    "tsdown": "^0.18.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DaikoAI/prettier-config.git"
