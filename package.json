{
  "name": "@daikolabs/prettier-config",
  "version": "1.0.1",
  "description": "Daiko Labs shared Prettier configuration",
  "type": "module",
  "exports": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "license": "MIT",
  "engines": {
    "node": ">=24.10.0"
  },
  "publishConfig": {
